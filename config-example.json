{
  "target": "map",
  "key": "YOUR_MAPTILER_API_KEY",
  "styleUrl": "https://api.maptiler.com/maps/streets/style.json",
  "showNavigationControls": true,
  "controlZoom": false,
  "minZoom": 3,
  "maxBounds": [
    [-180, -85],
    [180, 85]
  ],
  "showLayers": true,
  "layersTitle": "Layers",
  "showProximitySearch": true,
  "markerImage": "/dist/img/marker.svg",
  "showResults": true,
  "resultsTitle": "Results",
  "featuredTitle": "Featured",
  "featuredHeaderColor": "#333",
  "plugins": {
    "enabled": true,
    "defaultFeatures": {
      "kmz-upload": false,
      "address-upload": false,
      "multi-address-search": false,
      "address-recording": false,
      "building-styles": false,
      "search-card": true
    },
    "allowUserOverride": true,
    "saveToLocalStorage": true
  }
}